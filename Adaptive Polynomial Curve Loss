e (s=50)\n\n# Annotate the clusters with the corresponding string labels\nfor i, label in enumerate(labels_np):\n    plt.text(embeddings_2d[i, 0], embeddings_2d[i, 1], label, fontsize=8, ha='right', va='bottom')\n\n# Add a color legend\nlegend1 = plt.legend(*scatter.legend_elements(), title=\"Person\")\nplt.gca().add_artist(legend1)\n\n# Set plot details\nplt.title('t-SNE of Image Embeddings with Cluster Labels')\nplt.xlabel('Component 1')\nplt.ylabel('Component 2')\n\n# Show the plot\nplt.show()\n","metadata":{"execution":{"iopub.status.busy":"2024-09-23T15:39:13.598706Z","iopub.execute_input":"2024-09-23T15:39:13.599607Z","iopub.status.idle":"2024-09-23T15:39:15.406574Z","shell.execute_reply.started":"2024-09-23T15:39:13.599543Z","shell.execute_reply":"2024-09-23T15:39:15.404802Z"},"trusted":true},"execution_count":1,"outputs":[{"traceback":["\u001b[0;31m---------------------------------------------------------------------------\u001b[0m","\u001b[0;31mNameError\u001b[0m                                 Traceback (most recent call last)","Cell \u001b[0;32mIn[1], line 14\u001b[0m\n\u001b[1;32m      8\u001b[0m \u001b[38;5;66;03m# Sample image embeddings and labels\u001b[39;00m\n\u001b[1;32m      9\u001b[0m \u001b[38;5;66;03m# embeddings = ... (your embeddings, e.g., a NumPy array of shape [n_samples, embedding_dim])\u001b[39;00m\n\u001b[1;32m     10\u001b[0m \u001b[38;5;66;03m# labels_np = ... (your string labels, e.g., a list or array of labels corresponding to the embeddings)\u001b[39;00m\n\u001b[1;32m     11\u001b[0m \n\u001b[1;32m     12\u001b[0m \u001b[38;5;66;03m# Convert string labels to numeric labels\u001b[39;00m\n\u001b[1;32m     13\u001b[0m label_encoder \u001b[38;5;241m=\u001b[39m LabelEncoder()\n\u001b[0;32m---> 14\u001b[0m numeric_labels \u001b[38;5;241m=\u001b[39m label_encoder\u001b[38;5;241m.\u001b[39mfit_transform(\u001b[43mlabels_np\u001b[49m)\n\u001b[1;32m     16\u001b[0m \u001b[38;5;66;03m# Use t-SNE to reduce the embedding space to 2D for visuali